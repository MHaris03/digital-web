{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "skyliftgroup.com" }],
      "destination": "https://www.skyliftgroup.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
